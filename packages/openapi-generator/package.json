{
  "name": "@boilerstone/openapi-generator",
  "version": "0.0.1",
  "private": true,
  "main": "./index.ts",
  "scripts": {
    "dev": "dotenvx run -- wait-on ${API_URL}/docs-json && pnpm generate --watch",
    "preprocess": "node ./preprocess/index.js",
    "generate": "cross-env dotenvx run -- pnpm preprocess && dotenvx run -- openapi-ts -f ./openapi-ts.config.ts"
  },
  "peerDependencies": {
    "zod": "catalog:validation"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@hey-api/openapi-ts": "^0.91.0",
    "cross-env": "^10.1.0",
    "node-fetch": "^3.3.2",
    "wait-on": "^9.0.3"
  }
}
