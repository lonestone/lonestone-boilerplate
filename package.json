{
  "name": "boilerstone",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "description": "Monorepo for Lonestone project",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "engines": {
    "node": "24.13.0",
    "pnpm": "10.28.0"
  },
  "scripts": {
    "rock": "ts-node ./cli/setup.ts",
    "dev": "pnpm --parallel --filter=!@boilerstone/mobile dev",
    "dev:mobile": "pnpm --filter=@boilerstone/mobile dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "generate": "pnpm --filter=@boilerstone/openapi-generator run generate",
    "test": "pnpm -r test",
    "schematics:module": "pnpm --filter=@boilerstone/schematics run schematics:module",
    "docs-only": "pnpm --filter=@boilerstone/documentation run dev",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prepare": "husky"
  },
  "dependencies": {
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.1",
    "@eslint-react/eslint-plugin": "^2.6.4",
    "enquirer": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
