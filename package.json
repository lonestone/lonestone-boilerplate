{
  "name": "boilerstone",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "description": "Monorepo for Lonestone project",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "engines": {
    "node": "24.10.0",
    "pnpm": "10.5.2"
  },
  "scripts": {
    "rock": "ts-node ./cli/setup.ts",
    "dev": "pnpm --parallel --filter=!@boilerstone/mobile dev",
    "dev:mobile": "pnpm --filter=@boilerstone/mobile dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "generate": "pnpm --filter=@boilerstone/openapi-generator run generate",
    "test": "pnpm -r test",
    "schematics:module": "pnpm --filter=@boilerstone/schematics run schematics:module",
    "docs-only": "pnpm --filter=@boilerstone/documentation run dev",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prepare": "husky"
  },
  "dependencies": {
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@eslint-react/eslint-plugin": "^1.52.6",
    "enquirer": "^2.4.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0"
    }
  }
}
