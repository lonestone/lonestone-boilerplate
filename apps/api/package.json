    "@elysiajs/eden": "^1.4.8",
    "@types/bun": "^1.2.5",
    "@types/lodash": "^4.17.23",
    "@types/node": "catalog:build",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.4",
    "typescript": "catalog:build",
    "vitest": "^4.0.18"
  }
}
