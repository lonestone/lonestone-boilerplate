{
  "name": "@boilerstone/api",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "bun run --watch src/main.ts",
    "build": "bun build src/main.ts --outdir=dist --target=bun",
    "start": "bun run dist/main.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:seed": "bun run src/modules/db/db.seed.ts",
    "test": "cross-env NODE_ENV=test vitest run --config=vitest.config.ts",
    "test:watch": "cross-env NODE_ENV=test vitest --config=vitest.config.ts --watch",
    "test:e2e": "vitest run --project=e2e",
    "test:unit": "vitest run --project=unit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.29",
    "@ai-sdk/google": "^3.0.16",
    "@ai-sdk/mcp": "^1.0.14",
    "@ai-sdk/mistral": "^3.0.14",
    "@ai-sdk/openai": "^3.0.21",
    "@dotenvx/dotenvx": "^1.52.0",
    "@elysiajs/cors": "^1.3.1",
    "@elysiajs/openapi": "^1.4.14",
    "@elysiajs/static": "^1.3.1",
    "@elysiajs/stream": "^1.1.0",
    "@elysiajs/swagger": "^1.3.1",
    "@faker-js/faker": "^10.2.0",
    "@langfuse/client": "^4.5.1",
    "@langfuse/otel": "^4.5.1",
    "@langfuse/tracing": "^4.5.1",
    "@opentelemetry/sdk-trace-node": "^2.5.0",
    "@sentry/bun": "^10.37.0",
    "@sentry/opentelemetry": "^10.37.0",
    "@testcontainers/postgresql": "^11.11.0",
    "ai": "^6.0.59",
    "better-auth": "catalog:auth",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.0",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.7.0",
    "elysia": "^1.3.1",
    "nodemailer": "^7.0.13",
    "pg": "^8.17.2",
    "pino": "10.3.0",
